<script>
        let selectedImages = [];
    // Trigger the hidden file input when the custom image is clicked
    document.getElementById('custom-file-trigger').addEventListener('click', function() {
        document.getElementById('review_images').click();
    });

       // Form submission logic
    function submitReview() {
        if (!$('.add-review-progress').hasClass('d-none'))
            return;

        const files = selectedImages; // Use the updated selectedImages array

        if (files.length > 3) {
            $('.image-warning').removeClass('d-none');
            return;
        }
        $('.image-warning').addClass('d-none');

        $('.add-review-progress').removeClass('d-none');

        let formData = new FormData();
        formData.append('comment', $('.input-review-comment').val());
        formData.append('rating', $("#selected_rating").val());
        formData.append('is_anonymous', $('#is_review_customer_anonymous').is(':checked'));

        for (let i = 0; i < files.length; i++) {
            formData.append(`images[${i}][file]`, files[i]);
        }

        // Submit the review with the image files
        zid.store.product.addReview('debb65a8-690c-4c78-856d-1e8bce9666ed', formData).then(function(response) {
            if (response.status === 'success') {
                toastr.success('', 'تم الإرسال بنجاح');
            } else {
                toastr.error(response.data.message, 'عفوا');
            }
            $('.add-review-progress').addClass('d-none');
            $('#exampleModalCenter').modal('hide');
        });
    }
    function updateImagePreview() {
        const previewContainer = document.getElementById('image_preview');
        previewContainer.innerHTML = '';

        // Loop through the updated selectedImages array and re-render previews
        selectedImages.forEach((file, index) => {
            const reader = new FileReader();
            
            reader.onload = function(e) {
                const imgElement = document.createElement('div');
                imgElement.classList.add('image-preview-container');
                imgElement.innerHTML = `
                    <div class="position-relative">
                        <img src="${e.target.result}" class="img_thumbnail" alt="Image Preview">
                        <span class="remove-image position-absolute text-danger end-0 "  data-index="${index}">&times;</span>
                    </div>
                `;
                previewContainer.appendChild(imgElement);
            }

            reader.readAsDataURL(file); // Read file as URL to display it as an image
        });

        // Update validation and disable submit button if more than 3 images are selected
        if (selectedImages.length > 3) {
            $('.image-warning').removeClass('d-none');
            $('.btn-submit-new-review').prop('disabled', true);
        } else {
            $('.image-warning').addClass('d-none');
            $('.btn-submit-new-review').prop('disabled', false);
        }
    }
    document.addEventListener("DOMContentLoaded", function(){
          document.getElementById('review_images').addEventListener('change', function() {
                const files = Array.from(this.files); // Convert FileList to Array
                const previewContainer = document.getElementById('image_preview');

                // Clear previous previews
                previewContainer.innerHTML = '';
                selectedImages = files; // Update the selectedImages array with current files

                // If more than 3 images are selected, show warning and disable submit
                if (selectedImages.length > 3) {
                    $('.image-warning').removeClass('d-none');
                    $('.btn-submit-new-review').prop('disabled', true);
                } else {
                    $('.image-warning').addClass('d-none');
                    $('.btn-submit-new-review').prop('disabled', false);
                }

                // Loop through the selected files and create a preview with remove buttons
                selectedImages.forEach((file, index) => {
                    const reader = new FileReader();
                    
                    reader.onload = function(e) {
                        const imgElement = document.createElement('div');
                        imgElement.classList.add('image-preview-container');
                        imgElement.innerHTML = `
                            <div class="position-relative ">
                                <img src="${e.target.result}" class="img_thumbnail" alt="Image Preview">
                                <span class="remove-image position-absolute text-danger end-0" data-index="${index}">&times;</span>
                            </div>
                        `;
                        previewContainer.appendChild(imgElement);
                    }

                    reader.readAsDataURL(file); // Read file as URL to display it as an image
                });
        });
        // Handle image removal
        document.getElementById('image_preview').addEventListener('click', function(e) {
            if (e.target.classList.contains('remove-image')) {
                const indexToRemove = e.target.getAttribute('data-index');
                
                // Remove the image from the selectedImages array
                selectedImages.splice(indexToRemove, 1);

                // Trigger the change event again to update the UI and validation
                updateImagePreview();
            }
        });
        $(".btnrating").on('click hover mouseenter',(function(e) {

            var previous_value = $("#selected_rating").val();

            var selected_value = $(this).attr("data-attr");
            $("#selected_rating").val(selected_value);

            for (var i = 1; i <= selected_value; ++i) {
                $("#rating-star-"+i).toggleClass('active');
                $("#rating-star-"+i).toggleClass('deactive');
            }

            for (var ix = 1; ix <= previous_value; ++ix) {
                $("#rating-star-"+ix).toggleClass('active');
                $("#rating-star-"+ix).toggleClass('deactive');
            }

        }));

        $('textarea.input-review-comment').keypress(function() {
            var remainingCount = (250 - this.value.length);
            if(remainingCount < 0)  {
                $('.review-remaining-count').html('<span style="color:red">'+remainingCount+'</span>');
                $('.btn-submit-new-review').prop('disabled', true);
            }else{
                $('.review-remaining-count').html(remainingCount);
                $('.btn-submit-new-review').prop('disabled', false);
            }
        });
    });

</script>
